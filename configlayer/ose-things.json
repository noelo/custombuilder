{
   "kind": "List",
   "apiVersion": "v1",
   "metadata": {
      "name": "springboot-full-image",
      "labels": {
         "custombuilder": "true"
      }
   },
   "items": [
                        {
         "kind": "BuildConfig",
         "apiVersion": "v1",
         "metadata": {
            "name": "springboot-full-image"
         },
         "spec": {
            "source": {
               "type": "Git",
               "git": {
                  "uri": "https://github.com/noelo/custombuilder.git",
                  "ref": "master"
               },"contextDir": "configlayer/"
            },
            "strategy": {
			    "dockerStrategy":{
			   		"from": {
		                  "kind": "ImageStreamTag",
		                  "name": "springboot-app-image:latest"
		               },
					   "noCache": true
		        },
               "type": "Docker"
            },
             "output": {
               "to": {
                  "kind": "ImageStreamTag",
                  "name": "springboot-full-image:latest"
               }
            },
            "resources": {}
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "springboot-full-image"
         },
         "spec": {
            "dockerImageRepository": "",
            "tags": [
               {
                  "name": "latest"
               }
            ]
         }
      }
  ]
  }
